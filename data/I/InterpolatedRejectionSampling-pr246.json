      "author": {
        "login": "m-wells"
      },
      "authorAssociation": "NONE",
      "body": "[done](https://github.com/JuliaLang/Pkg.jl/pull/1175)",
      "createdAt": "2019-04-25T13:52:46Z",
      "includesCreatedEdit": false,
      "isMinimized": false,
